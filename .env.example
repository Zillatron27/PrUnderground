# PrUnderground Configuration

# Secret key for session signing (generate with: python -c "import secrets; print(secrets.token_hex(32))")
SECRET_KEY=your-secret-key-here

# Session secret for signed cookies (generate with: python -c "import secrets; print(secrets.token_hex(32))")
SESSION_SECRET=your-session-secret-here

# CSRF secret for form protection (generate with: python -c "import secrets; print(secrets.token_hex(32))")
CSRF_SECRET=your-csrf-secret-here

# Database URL (SQLite by default)
DATABASE_URL=sqlite:///./prununderground.db

# FIO API base URL
FIO_API_BASE=https://rest.fnar.net
